local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Pon hub", "DarkTheme")

-- MAIN
local Main = Window:NewTab("Інші скріпти")
local MainSection = Main:NewSection("Інші скріпти")

MainSection:NewButton("Infinity Yield", "скріпт на адміністратора", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
end)

MainSection:NewButton("Cmd X", "скріпт на адміністратора", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/CMD-X/CMD-X/master/Source"))()
end)

MainSection:NewButton("Owl Hub", "звичайний вх і аімбот", function()
     loadstring(game:HttpGet(("https://raw.githubusercontent.com/CriShoux/OwlHub/master/OwlHub.txt"),true))()
end)

MainSection:NewButton("Keyboard(mobile)", "клавіатура для телефону", function()
     loadstring(game:HttpGet(("https://raw.githubusercontent.com/advxzivhsjjdhxhsidifvsh/mobkeyboard/main/main.txt"),true))()
end)

local Main = Window:NewTab("Гравець")
local MainSection = Main:NewSection("Гравець")

MainSection:NewToggle("Бескинечний Стрибок", "Стрибни", function()
        local InfiniteJumpEnabled = true
        game:GetService("UserInputService").JumpRequest:connect(function()
            if InfiniteJumpEnabled then
                game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
            end
        end)
    end)

    MainSection:NewSlider("Швидкість", "Швидкість", 500, 16, function(s)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
    end)

    MainSection:NewSlider("СилаСтрибка", "Стрибни", 500, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
    end)

MainSection:NewButton("Noclip(нажміть n)", "ходи через стіни", function()
     loadstring(game:HttpGet(("https://raw.githubusercontent.com/prosko1234/noclip/main/noclip.lua"),true))()
end)

MainSection:NewButton("Fly(нажміть e)", "літай безкоштовно", function()
     loadstring(game:HttpGet(("https://raw.githubusercontent.com/prosko1234/Fly/main/script.lua"),true))()
end)

